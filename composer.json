{
    "name": "coliving/awesome-helpers",
    "description": "Helper functions I find super-duper handy. ",
    "keywords": [
        "calebporzio",
        "coliving",
        "awesome-helpers"
    ],
    "homepage": "https://github.com/dungnh/awesome-helpers",
    "license": "MIT",
    "authors": [
        {
            "name": "Dzung Nguyen",
            "email": "dungnh@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0 || ^8.1 || ^8.2 || ^8.3 || ^8.4"
    },
    "require-dev": {
        "fakerphp/faker": "^1.12",
        "illuminate/support": "5.7.*|5.8.*|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
        "mockery/mockery": "^1.6",
        "phpunit/phpunit": "^8.0|^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Calebporzio\\AwesomeHelpers\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Calebporzio\\AwesomeHelpers\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Calebporzio\\AwesomeHelpers\\AwesomeHelpersServiceProvider"
            ]
        }
    }
}
